{
  "manifest_version": 2,
  "name": "GatePeeper",
  "version": "1.0",
  "description": "Get more information about GateKeeper tracffic.",
  "content_scripts": [
    {
      "matches": ["http://developer.phila.gov/reports/top-users*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "browser_action": {
    "default_icon": "img/icon-32.png"
  }
}
